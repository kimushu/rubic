    "delay": "^1.3.1",
    "electron-settings": "^2.2.2",
    "event-kit": "^2.2.0",
    "github-api": "2.2.0",
    "handlebars": "^4.0.6",
    "i18n": "^0.8.3",
    "js-yaml": "^3.6.1",
    "jszip": "3.0.0",
    "node-uuid": "^1.4.7",
    "once": "^1.4.0",
    "sprintf-js": "^1.0.3"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "browserify": "^13.0.1",
    "coffee-script": "^1.12.4",
    "coffeeify": "^2.0.1",
    "decompress": "^4.0.0",
    "electron": "1.6.1",
    "electron-packager": "^8.5.2",
    "fs-extra": "^2.0.0",
    "less": "^2.7.1",
    "less-plugin-cache": "^1.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "npm-run-all": "^4.0.2",
    "recursive-readdir": "^2.1.1",
    "request": "^2.80.0",
    "request-promise": "^4.1.1",
    "rimraf": "^2.6.1",
    "uglifyjs": "^2.4.10",
    "watch": "^1.0.2",
    "yuidocjs": "^0.10.2"
  },
  "main": "lib/main/app/start.js",
  "author": "kimu_shu",
  "scripts": {
    "start": "electron . --dev-tools",
    "start:repeat": "npm run start && npm run start:repeat &",
    "clean": "rimraf lib static",
    "build": "npm-run-all -p build:*",
    "build:js": "coffee -M -o lib -c src",
    "build:html": "coffee view/build.coffee",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "watch 'npm run build:js' src -d",
    "watch:html": "watch 'npm run build:html' view -d",
    "predoc": "npm run build:js",
    "doc": "yuidoc -o doc lib"
  }
}
