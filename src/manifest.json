{
  "manifest_version": 2,
  "name": "PERIDOT Sketch",
  "version": "0.1.14.1229",
  /*"default_locale": "jp",*/
  "description": "Sketch IDE for PERIDOT",
  "icons": {
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "serial",
    {"fileSystem": ["directory", "write"]}
  ]/*,
  "content_security_policy": [
    {"script-src": ["self", "unsafe-eval"]}
  ]*/
}
